<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fi.group11.plantscare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PlantsCare_Group11">
        <activity android:name=".AddPlantFromUser"></activity>
        <activity android:name=".AddPlantFromDatabase" />
        <activity android:name=".AddPlantActivity" />
        <activity android:name=".HistoryActivity" />
        <activity android:name=".ReminderActivity" />
        <activity android:name=".MyPlantActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>